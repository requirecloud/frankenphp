{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": ": "
  },
  "modules": [
    "os",
    {
      "type": "command",
      "key": "PHP",
      "text": "php -r 'echo phpversion();'"
    },
    {
      "type": "command",
      "key": "Composer",
      "text": "composer --version | awk '{print $3}'"
    },
    "kernel",
    "uptime",
    "shell",
    "localip"
  ]
}
