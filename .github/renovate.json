{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": ["main"],
  "dependencyDashboard": true,
  "enabled": true,
  "regexManagers": [
    {
      "fileMatch": ["^docker-bake\\.hcl$"],
      "matchStrings": [
        "frankenphp_upstream = \"docker-image://(?<depName>.*?):(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
